{
  "schema_version": 1,
  "mixin": [
    "translucencyfix-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "translucencyfix",
    "version": "${version}",
    "metadata": {
      "name": "Translucency Fix",
      "description": "enables translucency for HUD and GUIs in Minecraft",
      "authors": [
        "ruvaldak"
      ],
      "contact": {
        "sources": "https://github.com/ruvaldak/translucencyfix"
      },
      "license": "MIT",
      "icon": "assets/translucencyfix/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
    },
    "depends": [
      {
        "id": "minecraft",
        "version": ">=1.20.0"
      }
    ]
  }
}